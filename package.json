{
  "name": "iskilog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest src",
    "test:ui": "vitest --ui src",
    "test:run": "vitest run src",
    "e2e": "playwright test",
    "e2e:flow1": "playwright test -g \"flow 1:\"",
    "e2e:rest": "playwright test --grep-invert \"flow 1:\"",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:db:prepare": "node tests/e2e/scripts/db-prepare.mjs",
    "e2e:db:cleanup": "node tests/e2e/scripts/db-cleanup.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "dotenv": "^16.6.1",
    "pg": "^8.16.3",
    "supabase": "^2.76.8",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.7.0"
  }
}
